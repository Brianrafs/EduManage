{
    "version": 2,
    "builds": [
      {
        "src": "dist/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "YOUR_ENV_VARIABLE": "value"
    },
    "functions": {
      "maxLambdaSize": "2mb"
    }
  }
  